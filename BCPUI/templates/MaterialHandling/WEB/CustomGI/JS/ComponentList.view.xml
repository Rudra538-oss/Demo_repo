	xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" controllerName="JS.ComponentList"
	xmlns:f="sap.ui.layout.form" xmlns:table="sap.ui.table" xmlns:html="http://www.w3.org/1999/xhtml">
